{
  "manifest_version": 3,
  "name": "PhishBlock",
  "version": "1.0",
  "description": "Checks the current URL for phishing risk.",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; style-src 'self' https://cdnjs.cloudflare.com;"
  }
}